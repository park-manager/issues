{
    "name": "park-manager/park-manager",
    "description": "Open-source hosting solution",
    "keywords": ["ftp", "mail", "server", "hosting"],
    "type": "project",
    "license": "MPL-2.0",
    "homepage": "http://www.park-manager.com/",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "role": "Project Leader",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/symfony": "^3.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3.8",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "aequasi/environment": "^2.0"
    },
    "require-dev": {
        "behat/behat": "^3.1",
        "behat/symfony2-extension": "^2.1",
        "behat/mink-extension": "^2.1",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink": "^1.7",
        "matthiasnoback/symfony-service-definition-validator": "^1.2",
        "sensiolabs/behat-page-object-extension": "~2.0@rc",
        "symfony/phpunit-bridge": "^3.0",
        "sllh/php-cs-fixer-styleci-bridge": "^2.0"
    },
    "replace": {
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*"
    },
    "autoload": {
        "psr-4": {
            "ParkManager\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
