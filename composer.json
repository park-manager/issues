{
    "name": "park-manager/park-manager",
    "type": "project",
    "description": "Open-source hosting solution",
    "keywords": [
        "ftp",
        "mail",
        "server",
        "hosting"
    ],
    "homepage": "http://www.park-manager.com/",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/park-manager/park-manager/contributors"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-mbstring": "*",
        "ext-sodium": "*",
        "beberlei/assert": "^3.0.0",
        "doctrine/dbal": "^2.10",
        "doctrine/doctrine-bundle": "^2.0.2",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "egulias/email-validator": "^2.1",
        "knplabs/knp-menu-bundle": "^3.0.0",
        "paragonie/constant_time_encoding": "^2.0",
        "paragonie/halite": "^4.4.2",
        "ramsey/uuid": "^4.0",
        "rollerworks/route-autofill-bundle": "^0.1.1",
        "rollerworks/split-token": "^0.1.0",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.3.0",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/mailer": "*",
        "symfony/messenger": "*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/security-bundle": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/webpack-encore-bundle": "*",
        "symfony/yaml": "*"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0.0",
        "phpunit/phpunit": "^8.2.3",
        "psalm/plugin-phpunit": "^0.10",
        "slevomat/coding-standard": "^6.2.0",
        "squizlabs/php_codesniffer": "^3.5.4",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/panther": "^0.7.1",
        "symfony/phpunit-bridge": "*",
        "symfony/stopwatch": "*",
        "symfony/var-dumper": "*",
        "symfony/web-profiler-bundle": "*",
        "vimeo/psalm": "^3.0",
        "weirdan/doctrine-psalm-plugin": "^0.11"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        },
        "symfony": {
            "id": "01BF6RBYGY5V4S3FDMBNKXVVNS",
            "require": "^5.1",
            "allow-contrib": true
        }
    },
    "autoload": {
        "psr-4": {
            "ParkManager\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ParkManager\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
